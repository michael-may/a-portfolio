{{#withDocpad document}}
<!doctype html>
<html>
	<head>
		<title>Alyssa May Illustration</title>
		<link rel="stylesheet" type="text/css" href="/styles/global.css">
		<meta name="viewport" content="width=device-width, initial-scale=1">
		<!-- Google tag (gtag.js) -->
		<script async src="https://www.googletagmanager.com/gtag/js?id=G-MK8FNCPJKJ"></script>
		<script>
			window.dataLayer = window.dataLayer || [];
			function gtag(){dataLayer.push(arguments);}
			gtag('js', new Date());

			gtag('config', 'G-MK8FNCPJKJ');
		</script>
	</head>
	<body>
		<nav class="main-nav project">
			<a href="mailto:alyssa@artofalyssamay.com" class="contact">
				<img src="/img/contact.svg">
			</a>
			<div id="logo">
				<a href="/">
					<img src="/img/SignatureVector.svg">
				</a>
			</div>
			<div id="logo-bg">
				<img src="/img/logo_bg2x.png">
			</div>
		</nav>
		<div id="main" class="project">
			<section id="images">
				{{#compare imgRatio 'portrait' operator='=='}}
					<div id="img-main" class="portrait img">
						<img src="/img/projects/{{imgLarge}}">
					</div>
				{{/compare}}
				{{#compare imgRatio 'portrait' operator='!='}}
					<div id="img-main" class="img">
						<img src="/img/projects/{{imgLarge}}">
					</div>
				{{/compare}}
				<div id="img-sub1" class="img-sub">
					<img src="/img/projects/{{imgDetail1}}">
				</div>
				<div id="img-sub2" class="img-sub">
					<img src="/img/projects/{{imgDetail2}}">
				</div>
			</section>
			<div class="clearfix"></div>
			<section id="content" class="floating-section">
				<div class="content-inner">
					<div class="content-left">
						<h1>{{title}}</h1>
						{{{md2html content}}}
						{{#if prints}}
							<div class="prints">
								<h3>Prints available at:</h3>
								{{#if redbubbleUrl}}
									<a href="{{redbubbleUrl}}">
										<img src="/img/redbubble.svg">
									</a>
								{{/if}}
								{{#if society6Url}}
									<a href="{{society6Url}}">
										<img src="/img/society6.svg">
									</a>
								{{/if}}
								{{#if cratedUrl}}
									<a href="{{cratedUrl}}">
										<img src="/img/crated.svg">
									</a>
								{{/if}}
							</div>
						{{/if}}
					</div>
					<div class="content-right">
						<h2>Process video:</h2>
						<a href="{{youtubeUrl}}">
							<img src="/img/projects/{{youtubeThumb}}">
						</a>
					</div>
				</div>
			</section>
			{{> about}}
			{{> footer}}
		</div>
		<!--Workaround for webkit bug-->
		<div id="background">
			<img src="/img/main_bg.jpg">
		</div>
	</body>
</html>
{{/withDocpad}}